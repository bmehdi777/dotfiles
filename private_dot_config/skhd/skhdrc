# Focus
cmd - h : yabai -m window --focus west
cmd - j : yabai -m window --focus south
cmd - k : yabai -m window --focus north
cmd - l : yabai -m window --focus east

# Move inside of space
shift + cmd - j : yabai -m window --swap south
shift + cmd - k : yabai -m window --swap north
shift + cmd - h : yabai -m window --swap west
shift + cmd - l : yabai -m window --swap east

# Move window between space
shift + alt + cmd - h : yabai -m window --space prev; yabai -m space --focus prev; 
shift + alt + cmd - l : yabai -m window --space next; yabai -m space --focus next; 
# move between space
alt + cmd - h : yabai -m space --focus prev 
alt + cmd - l : yabai -m space --focus next

# Move to numbered space
cmd - 0x12 : yabai -m space --focus 1  
cmd - 0x13 : yabai -m space --focus 2  
cmd - 0x14 : yabai -m space --focus 3  
cmd - 0x15 : yabai -m space --focus 4  
cmd - 0x1A : yabai -m space --focus 5  
cmd - 0x1C : yabai -m space --focus 6  
cmd - 0x19 : yabai -m space --focus 7  
cmd - 0x1D : yabai -m space --focus 8  
